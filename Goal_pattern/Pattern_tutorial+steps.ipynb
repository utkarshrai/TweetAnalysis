    "import csv\n",
    "with open(\"file_name.csv\",'wb') as f:\n",
    "    writer = csv.writer(f, dialect='excel')\n",
    "    for tweet in twitter.search('\"Ramjas\"', cached=False):\n",
    "        writer.writerow(tweet.text)"
   ]
